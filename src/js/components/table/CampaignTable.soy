{namespace CampaignTable}

/**
 * @param? handleTableClicked_
 * @param? tableData
 */
{template .render}
	<div class="campaign-manager-campaign-table">
		{call Table.render}
			{param headers: ['Name', 'Scheduled Date', 'Goal', 'Edit', 'Remove'] /}
			{param data: $tableData /}
			{param events: ['click': $handleTableClicked_] /}
			{param cssClass: 'table-box' /}
		{/call}
	</div>
{/template}
